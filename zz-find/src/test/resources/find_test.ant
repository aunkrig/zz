<project>

<!--	<taskdef file="../classes/de/unkrig/zz/zz-find.xml" />-->
	<taskdef file="target/classes/de/unkrig/zz/zz-find.xml" />

	<target name="find1">
		<zzfind lookInto="zip:**">
			<path path="files" />
			<name value="**file1" />
			<print />
		</zzfind>
	</target>

	<target name="find2">
		<zzfind lookInto="zip:***">
			<fileset dir="files" />
			<name value="**file1" />
			<and>
				<true />
				<or>
					<false />
					<true />
				</or>
			</and>
			<print />
		</zzfind>
	</target>
</project>
